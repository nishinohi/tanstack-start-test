{
  "name": "tanstack-test",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "CLOUDFLARE_ENV=local vite dev --port 3000",
    "build": "vite build",
    "build:preview": "rimraf dist && CLOUDFLARE_ENV=preview vite build --mode preview",
    "build:analyze": "ANALYZE=true vite build",
    "preview": "vite preview",
    "typecheck": "tsc --noEmit",
    "typegen:cf": "wrangler types",
    "test": "vitest run",
    "lint": "eslint",
    "format": "prettier",
    "check": "prettier --write . && eslint --fix",
    "db:generate": "drizzle-kit generate --config ./src/db/config/drizzle-local.config.ts",
    "db:migrate:local": "drizzle-kit migrate --config ./src/db/config/drizzle-local.config.ts",
    "db:migrate:preview": "drizzle-kit migrate --config ./src/db/config/drizzle-preview.config.ts",
    "db:migrate:dev": "drizzle-kit migrate --config ./src/db/config/drizzle-dev.config.ts",
    "db:migrate:stg": "drizzle-kit migrate --config ./src/db/config/drizzle-stg.config.ts",
    "db:migrate:prod": "drizzle-kit migrate --config ./src/db/config/drizzle-prod.config.ts",
    "db:view:local": "drizzle-kit studio --config ./src/db/config/drizzle-local.config.ts",
    "db:view:preview": "drizzle-kit studio --config ./src/db/config/drizzle-preview.config.ts",
    "db:view:dev": "drizzle-kit studio --config ./src/db/config/drizzle-dev.config.ts",
    "db:view:stg": "drizzle-kit studio --config ./src/db/config/drizzle-stg.config.ts",
    "db:view:prod": "drizzle-kit studio --config ./src/db/config/drizzle-prod.config.ts",
    "db:drop:local": "wrangler d1 execute tanstack-test-local --env local --local --file='./src/db/seed/drop-all-table.sql' --config=./wrangler.jsonc",
    "db:drop:preview": "wrangler d1 execute tanstack-test-preview --env preview --local --file='./src/db/seed/drop-all-table.sql' --config=./wrangler.jsonc",
    "db:drop:dev": "wrangler d1 execute tanstack-test-develop --env develop --remote --file='./src/db/seed/drop-all-table.sql' --config=./wrangler.jsonc",
    "db:drop:stg": "wrangler d1 execute tanstack-test-staging --env staging --remote --file='./src/db/seed/drop-all-table.sql' --config=./wrangler.jsonc",
    "db:drop:prod": "echo \"ðŸ”¥ Be careful ðŸ”¥\\nYou can delete all tables by executing:\\n> wrangler d1 execute tanstack-test --remote --file='./src/db/seed/drop-all-table.sql' --config=./wrangler.jsonc\""
  },
  "dependencies": {
    "@cloudflare/vite-plugin": "^1.17.1",
    "@hookform/resolvers": "^5.2.2",
    "@libsql/client": "^0.15.15",
    "@radix-ui/react-avatar": "^1.1.11",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-slot": "^1.2.4",
    "@tailwindcss/vite": "^4.1.18",
    "@tanstack/react-devtools": "^0.9.1",
    "@tanstack/react-query": "^5.90.16",
    "@tanstack/react-query-devtools": "^5.91.2",
    "@tanstack/react-router": "^1.145.7",
    "@tanstack/react-router-devtools": "^1.145.7",
    "@tanstack/react-router-ssr-query": "^1.145.7",
    "@tanstack/react-start": "^1.145.8",
    "@tanstack/router-plugin": "^1.145.7",
    "better-auth": "^1.4.10",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "dotenv": "^17.2.3",
    "drizzle-orm": "^0.45.1",
    "lucide-react": "^0.562.0",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "react-error-boundary": "^6.1.0",
    "react-hook-form": "^7.70.0",
    "tailwind-merge": "^3.4.0",
    "tailwindcss": "^4.1.18",
    "tw-animate-css": "^1.4.0",
    "vite-tsconfig-paths": "^6.0.3",
    "zod": "^4.3.5"
  },
  "devDependencies": {
    "@tanstack/devtools-vite": "^0.4.1",
    "@tanstack/eslint-config": "^0.3.4",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/react": "^16.3.2",
    "@trivago/prettier-plugin-sort-imports": "^6.0.1",
    "@types/node": "^24.10.4",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.2",
    "drizzle-kit": "^0.31.8",
    "eslint": "^10.0.0",
    "eslint-plugin-import-x": "^4.16.1",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "jsdom": "^27.4.0",
    "lefthook": "^2.0.13",
    "prettier": "^3.8.1",
    "prettier-plugin-tailwindcss": "^0.7.2",
    "rimraf": "^6.1.2",
    "rollup-plugin-visualizer": "^6.0.5",
    "stylelint": "^17.1.1",
    "stylelint-config-standard": "^40.0.0",
    "textlint": "^15.5.0",
    "textlint-rule-preset-ja-spacing": "^2.4.3",
    "textlint-rule-preset-ja-technical-writing": "^12.0.2",
    "textlint-rule-preset-jtf-style": "^3.0.3",
    "typescript": "^5.9.3",
    "vite": "^7.3.0",
    "vitest": "^4.0.18",
    "web-vitals": "^5.1.0",
    "wrangler": "^4.54.0"
  },
  "engines": {
    "node": "24.11.x"
  },
  "packageManager": "pnpm@10.27.0+sha512.72d699da16b1179c14ba9e64dc71c9a40988cbdc65c264cb0e489db7de917f20dcf4d64d8723625f2969ba52d4b7e2a1170682d9ac2a5dcaeaab732b7e16f04a"
}
